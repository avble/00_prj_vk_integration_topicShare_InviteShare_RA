<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_device_type_sensor_multilevel5"
    tools:context="remote.service.verik.com.remoteaccess.DeviceTypeSensorMultilevel6">


    <TextView
        android:id="@+id/sensor_multilevel5_device_name"
        android:layout_width="550px"
        android:layout_height="wrap_content"
        android:text="Z-Wave light bulb"
        android:layout_marginTop="35px"
        android:layout_marginLeft="20px"
        android:textSize="50px"/>

    <ImageView
        android:id="@+id/sensor_multilevel5_bulb"
        android:layout_width="100px"
        android:layout_height="100px"
        android:src="@drawable/bulb_off"
        android:layout_marginLeft="20px"
        android:layout_marginTop="10px"
        android:layout_marginBottom="10px"
        android:layout_toRightOf="@+id/sensor_multilevel5_device_name"
        />


    <ScrollView
        android:id="@+id/ScrlView"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/sensor_multilevel5_bulb">


    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:paddingTop="40dp"
        android:layout_below="@id/sensor_multilevel5_bulb">

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel5_tw_temp"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="Temp"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel5_button_temp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET TEMP"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>
        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel5_tw_humi"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="Humi"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel5_button_humi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET HUMI"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel5_tw_lumi"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="LUMI"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel5_button_lumi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET LUMI"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow31"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel5_tw_battery"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="BATTERY"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel5_button_battery"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET BATTERY"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>


        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15px" >

            <TextView
                android:id="@+id/sensor_multilevel5_tw_setting"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="SETTING"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>



        <TableRow
            android:id="@+id/tableRow8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10px" >

            <CheckBox android:id="@+id/sensor_multilevel5_cb_motion_enable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Enable motion " />


        </TableRow>




        <TableRow
            android:id="@+id/tableRow7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/sensor_multilevel5_tw_config_report"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Report configuration"
                    android:textAppearance="?android:attr/textAppearanceLarge"/>

                <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                        <RadioButton android:id="@+id/sensor_multilevel5_radio_temperature"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Temperature"
                            />

                        <RadioButton android:id="@+id/sensor_multilevel5_radio_humidity"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Humidity"
                            />

                        <RadioButton android:id="@+id/sensor_multilevel5_radio_luminance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Luminance"
                            />

                        <RadioButton android:id="@+id/sensor_multilevel5_radio_battery"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Battery"
                            />

                </RadioGroup>

            </LinearLayout>

        </TableRow>






        <TableRow
            android:id="@+id/tableRow9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/sensor_multilevel5_tw_auto_timer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Timer configuration"
                    android:textAppearance="?android:attr/textAppearanceLarge"/>

                <SeekBar
                    android:id="@+id/sensor_multilevel5_seekbar_auto_timer"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="10dp"
                    android:layout_width="500px"
                    android:max="10"/>

            </LinearLayout>

        </TableRow>

        <TableRow
            android:id="@+id/tableRow6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/sensor_multilevel5_tw_config_"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Auto Timer configuration"
                    android:textAppearance="?android:attr/textAppearanceLarge"/>

                <SeekBar
                    android:id="@+id/sensor_multilevel5_seekbar_configuration_timer"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="10dp"
                    android:layout_width="500px"
                    android:max="10"/>

                <!--<TextView-->
                <!--android:id="@+id/sensor_multilevel5_tw_timer"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_alignParentLeft="true"-->
                <!--android:layout_alignParentTop="true"-->
                <!--android:layout_marginTop="1dp"-->
                <!--android:layout_marginBottom="10dp"-->
                <!--android:layout_width="500px"-->
                <!--android:max="10"/>-->


            </LinearLayout>

        </TableRow>



        <TableRow
            android:id="@+id/tableRow511"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15px" >

            <TextView
                android:id="@+id/sensor_multilevel5_tw_association"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="ASSOCIATION"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow52"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >


            <CheckBox
                android:id="@+id/sensor_multilevel5_cb_association_add_group"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Enable Notification" />


        </TableRow>






    </TableLayout>

    </ScrollView>


</RelativeLayout>
