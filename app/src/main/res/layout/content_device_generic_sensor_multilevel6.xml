<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_device_type_sensor_multilevel6"
    tools:context="remote.service.verik.com.remoteaccess.DeviceTypeSensorMultilevel6">


    <TextView
        android:id="@+id/sensor_multilevel6_device_name"
        android:layout_width="550px"
        android:layout_height="wrap_content"
        android:text="Z-Wave light bulb"
        android:layout_marginTop="35px"
        android:layout_marginLeft="20px"
        android:textSize="50px"/>

    <ImageView
        android:id="@+id/sensor_multilevel6_bulb"
        android:layout_width="100px"
        android:layout_height="100px"
        android:src="@drawable/bulb_off"
        android:layout_marginLeft="20px"
        android:layout_marginTop="10px"
        android:layout_marginBottom="10px"
        android:layout_toRightOf="@+id/sensor_multilevel6_device_name"
        />


    <ScrollView
        android:id="@+id/ScrlView"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/sensor_multilevel6_bulb">


    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:paddingTop="40dp"
        android:layout_below="@id/sensor_multilevel6_bulb">

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel6_tw_temp"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="Temp"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel6_button_temp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET TEMP"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel6_tw_humi"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="Humi"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel6_button_humi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET HUMI"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel6_tw_lumi"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="LUMI"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel6_button_lumi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET LUMI"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel6_tw_ultra_violet"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="Violet"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel6_button_ultra_violet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET Ultra Vio"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15px" >

            <TextView
                android:id="@+id/sensor_multilevel6_tw_setting"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="SETTING"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>



        <TableRow
            android:id="@+id/tableRow51"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >


        <CheckBox
            android:id="@+id/sensor_multilevel6_cb_configuration_lock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Enable Configuration" />


        </TableRow>



        <TableRow
            android:id="@+id/tableRow6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/sensor_multilevel6_tw_config_"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Timer configuration"
                    android:textAppearance="?android:attr/textAppearanceLarge"/>

                <SeekBar
                    android:id="@+id/sensor_multilevel6_seekbar_configuration_timer"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="10dp"
                    android:layout_width="500px"
                    android:max="10"/>

                <!--<TextView-->
                    <!--android:id="@+id/sensor_multilevel6_tw_timer"-->
                    <!--android:layout_height="wrap_content"-->
                    <!--android:layout_alignParentLeft="true"-->
                    <!--android:layout_alignParentTop="true"-->
                    <!--android:layout_marginTop="1dp"-->
                    <!--android:layout_marginBottom="10dp"-->
                    <!--android:layout_width="500px"-->
                    <!--android:max="10"/>-->


            </LinearLayout>

        </TableRow>



        <TableRow
            android:id="@+id/tableRow7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10px" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/sensor_multilevel6_tw_config_report"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Report configuration"
                    android:textAppearance="?android:attr/textAppearanceLarge"/>


                        <CheckBox android:id="@+id/sensor_multilevel6_cb_temperature"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Temperature"
                            />

                        <CheckBox android:id="@+id/sensor_multilevel6_cb_humidity"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Humidity"
                            />

                        <CheckBox android:id="@+id/sensor_multilevel6_cb_luminance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Luminance"
                            />

                        <CheckBox android:id="@+id/sensor_multilevel6_cb_ultra_violet"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Ultra Violet"
                            />

                        <CheckBox android:id="@+id/sensor_multilevel6_cb_battery"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Battery"
                            />

            </LinearLayout>

        </TableRow>



        <TableRow
            android:id="@+id/tableRow8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10px" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/sensor_multilevel6_tw_config_motion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Motion configuration"
                    android:textAppearance="?android:attr/textAppearanceLarge"/>

                <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <RadioButton android:id="@+id/sensor_multilevel6_radio_motion_level1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Level 1"
                        />

                    <RadioButton android:id="@+id/sensor_multilevel6_radio_motion_level2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Level 2"
                        />

                    <RadioButton android:id="@+id/sensor_multilevel6_radio_motion_level3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Level 3"
                        />

                    <RadioButton android:id="@+id/sensor_multilevel6_radio_motion_level4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Level 4"
                        />

                    <RadioButton android:id="@+id/sensor_multilevel6_radio_motion_level5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Level 5"
                        />


                    <RadioButton android:id="@+id/sensor_multilevel6_radio_motion_disable"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Disable"
                        />

                </RadioGroup>

            </LinearLayout>

        </TableRow>



        <TableRow
            android:id="@+id/tableRow9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/sensor_multilevel6_tw_auto_timer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Timer configuration"
                    android:textAppearance="?android:attr/textAppearanceLarge"/>

                <SeekBar
                    android:id="@+id/sensor_multilevel6_seekbar_auto_timer"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="10dp"
                    android:layout_width="500px"
                    android:max="10"/>

            </LinearLayout>

        </TableRow>



        <TableRow
            android:id="@+id/tableRow511"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15px" >

            <TextView
                android:id="@+id/sensor_multilevel6_tw_association"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="ASSOCIATION"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow52"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >


            <CheckBox
                android:id="@+id/sensor_multilevel6_cb_association_add_group"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Enable Notification" />


        </TableRow>



    </TableLayout>

    </ScrollView>


</RelativeLayout>
