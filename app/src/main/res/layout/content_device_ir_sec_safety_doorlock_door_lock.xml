<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_device_type_sensor_ir_sec_safety_doorNlock">
    <!--tools:context="remote.service.verik.com.remoteaccess.DeviceTypeProtocol">-->



    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <!--android:layout_below="@id/sensor_ir_sec_safety_doorNlock_device_name">-->


        <TableRow
            android:id="@+id/tableRow5111"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15px" >

            <TextView
                android:id="@+id/sensor_ir_sec_safety_doorNlock_tw_door_lock"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="DOOR LOCK"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow521"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >


            <CheckBox
                android:id="@+id/sensor_ir_sec_safety_doorNlock_cb_door_lock"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Close" />

        </TableRow>


        <TableRow
            android:id="@+id/tableRow511"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15px" >

            <TextView
                android:id="@+id/sensor_ir_sec_safety_doorNlock_tw_association"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="ASSOCIATION"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow52"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >


            <CheckBox
                android:id="@+id/sensor_ir_sec_safety_doorNlock_cb_association_add_group"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Enable Notification" />

        </TableRow>


        <TableRow
            android:id="@+id/tableRow51111"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15px" >

            <TextView
                android:id="@+id/sensor_ir_sec_safety_doorNlock_tw_user_code"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="USER_CODE"
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow5211"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/sensor_ir_sec_safety_doorNlock_et_user_code_1"
                    android:layout_width= "300px"
                    android:layout_height="wrap_content"
                    android:hint="Len of pin (6-10) "/>


                <Button
                    android:id="@+id/sensor_ir_sec_safety_doorNlock_button_user_code_1_set"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="1px"
                    android:text="SET"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    />


            </LinearLayout>

        </TableRow>

    </TableLayout>

</RelativeLayout>
