<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_device_type_sensor_multilevel6"
    tools:context="remote.service.verik.com.remoteaccess.DeviceTypeSensorMultilevel6">



    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel6_multilevel_tw_temp"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="Temp"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel6_multilevel_button_temp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET TEMP"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel6_multilevel_tw_humi"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="Humi"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel6_multilevel_button_humi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET HUMI"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel6_multilevel_tw_lumi"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="LUMI"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel6_multilevel_button_lumi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET LUMI"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="1px" >

            <TextView
                android:id="@+id/sensor_multilevel6_multilevel_tw_ultra_violet"
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="Violet"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <Button
                android:id="@+id/sensor_multilevel6_multilevel_button_ultra_violet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="1px"
                android:text="GET Ultra Vio"
                android:textAppearance="?android:attr/textAppearanceSmall"
                />

        </TableRow>


    </TableLayout>

</RelativeLayout>
