<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_device_type_sensor_ir_sec_safety_aeotec_door_window">
    <!--tools:context="remote.service.verik.com.remoteaccess.DeviceTypeProtocol">-->


    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:paddingTop="1dp">
        <!--android:layout_below="@id/sensor_ir_sec_safety_aeotec_door_window_device_name">-->


        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px">

            <TextView
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="Basic Set.........."
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>


        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10px">

            <LinearLayout android:orientation="horizontal">

                <Button
                    android:id="@+id/sensor_door_window_basic_set_get"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="GET" />

                <CheckBox
                    android:id="@+id/sensor_cb_basic_set"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Basic Set" />


            </LinearLayout>
        </TableRow>


        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px">

            <TextView
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="Binary Set.........."
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>


        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10px">

            <LinearLayout android:orientation="horizontal">

                <Button
                    android:id="@+id/sensor_door_window_binary_set_get"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="GET" />

                <CheckBox
                    android:id="@+id/sensor_cb_binary_set"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Binary sensor" />


            </LinearLayout>
        </TableRow>


        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="1px">

            <TextView
                android:layout_width="500px"
                android:layout_height="wrap_content"
                android:text="DETERMINES.........."
                android:textAppearance="?android:attr/textAppearanceLarge" />

        </TableRow>


        <TableRow
            android:id="@+id/tableRow82"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10px">


        </TableRow>


        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10px"
            android:paddingLeft="40px">

            <LinearLayout android:orientation="vertical">

                <Button
                    android:id="@+id/sensor_door_window_determines_get"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="GET" />

                <CheckBox
                    android:id="@+id/sensor_door_window_button_determines_basic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Basic Set" />

                <CheckBox
                    android:id="@+id/sensor_door_window_button_determines_binary"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Binary Sensor" />

            </LinearLayout>
        </TableRow>


    </TableLayout>

</RelativeLayout>
